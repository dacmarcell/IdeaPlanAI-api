{
  "name": "ideaplan-ai",
  "version": "0.0.1",
  "main": "src/app.ts",
  "type": "module",
  "scripts": {
    "start:dev": "node --experimental-strip-types src/app.ts",
    "start:prod": "node dist/app.js",
    "build": "tsc && tsc-alias",
    "art:quick": "artillery quick --count 10 -n 20 http://localhost:3000",
