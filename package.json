  "description": "",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "openai": "^4.68.1",
    "pg": "^8.13.1",
    "pg-promise": "^11.10.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.8",
    "@types/pg": "^8.11.10",
    "@types/pg-promise": "^0.0.28",
    "artillery": "^2.0.26",
    "tsc-alias": "1.8.10",
    "typescript": "^5.6.3"
  }
